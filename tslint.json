{
  "extends": "tslint:recommended",
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "prefer-const": true,
    "object-literal-sort-keys": false,
    "no-require-imports": false,
    "no-var-requires": false,
    "eofline": false,
    "no-var-keyword": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      false
    ],
    "arrow-parens": false,
    "ordered-imports": [
      false
    ]
  }
}